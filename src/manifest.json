{
    "manifest_version": 3,
    "name": "Lattice Tabs - AI Tab Manager",
    "version": "0.6.0",
    "description": "AI-powered tab organization with privacy-first design.",
    "key": "",
    "permissions": [
        "storage",
        "tabs",
        "tabGroups",
        "alarms"
    ],
    "action": {
        "default_popup": "src/sidepanel/index.html",
        "default_title": "Open Lattice",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "background": {
        "service_worker": "background/index.js",
        "type": "module"
    },
    "options_ui": {
        "page": "src/options/index.html",
        "open_in_tab": true
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}